<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BlazorHybridMessAround.Views.DogsPage"
             xmlns:razorPages="clr-namespace:BlazorHybridMessAround.Components.Pages"
             Title="Dogs Page">
    <OnPlatform x:TypeArguments="View">

        <On Platform="WinUI">
            <BlazorWebView x:Name="BlazorWebViewWindows" HostPage="wwwroot/index.html">
                <BlazorWebView.RootComponents>
                    <RootComponent x:Name="BlazorWebViewRootComponentWindows" Selector="#app" ComponentType="{x:Type razorPages:Weather}" />
                </BlazorWebView.RootComponents>
            </BlazorWebView>
        </On>
    </OnPlatform>
</ContentPage>