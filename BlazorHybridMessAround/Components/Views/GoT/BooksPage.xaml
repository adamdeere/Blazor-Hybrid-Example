<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BlazorHybridMessAround.Components.Views.GoT.BooksPage"
             Title="G.o.T Books Page"
             Shell.FlyoutBehavior="Disabled"
             Shell.NavBarIsVisible="False"
             xmlns:razorPages="clr-namespace:BlazorHybridMessAround.Components.Pages.GoT">
    <BlazorWebView HostPage="wwwroot/index.html">
        <BlazorWebView.RootComponents>
            <RootComponent Selector="#app" ComponentType="{x:Type razorPages:GotBooksRazor}" />
        </BlazorWebView.RootComponents>
    </BlazorWebView>
</ContentPage>